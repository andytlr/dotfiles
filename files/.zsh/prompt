project_pwd() {
  echo $PWD | sed -e "s/\/Users\/$USER/~/" -e "s/~\/projects\/\([^\/]*\)\/current/\\1 :/" -e "s/~\/Sites\/\([^\/]*\)\/current/http:\/\/\\1 :/"
}

export PROMPT=$'%n%{\e[0;90m%}@%{\e[0m%}%m %{\e[0;%(?.32.31)m%}âœª %{\e[0m%}'
export RPROMPT=$'`project_pwd``~/.dotfiles/files/.bin/git_cwd_info`'
